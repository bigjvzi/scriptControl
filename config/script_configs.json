{
  "表格转翻译配置": {
    "module": "concat",
    "description": "表格转翻译配置",
    "parameters": [
      {
        "name": "input_folder",
        "label": "表格本地路径",
        "type": "file"
      },
      {
        "name": "remote_folder",
        "label": "目标路径",
        "type": "folder"
      },
      {
        "name": "file_name",
        "label": "导出文件名,多个导出文件用逗号分隔,下列参数保持一致",
        "type": "string"
      },
      {
        "name": "delimiter",
        "label": "连接符",
        "type": "string"
      },
      {
        "name": "encoding",
        "label": "value格式",
        "type": "string"
      },
      {
        "name": "end_symbol",
        "label": "结尾符号",
        "type": "string"
      }
    ]
  },
  "扫描中文": {
    "module": "scanChinese",
    "description": "扫描目标目录下指定文件的中文内容，并输出结果。",
    "preview": true,
    "parameters": [
      {
        "name": "input_folder",
        "label": "输入文件路径",
        "type": "folder",
        "required": true
      }
    ]
  },
  "同步代码到服务器": {
    "module": "syncGitFilesToRemote",
    "description": "将本地代码同步到远程服务器。",
    "parameters": [
      {
        "name": "input_folder",
        "label": "本地路径",
        "type": "folder"
      },
      {
        "name": "delete_path",
        "label": "忽略目录",
        "type": "string"
      },
      {
        "name": "remote_server",
        "label": "地址",
        "type": "string"
      },
      {
        "name": "prot",
        "label": "端口",
        "type": "string"
      },
      {
        "name": "username",
        "label": "用户名",
        "type": "string"
      },
      {
        "name": "password",
        "label": "密码",
        "type": "string"
      },
      {
        "name": "remote_folder",
        "label": "目标路径",
        "type": "string"
      }
    ]
  }
}
